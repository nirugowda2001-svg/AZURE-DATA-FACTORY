{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-dataproject1"
		},
		"DataProjectDatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataProjectDatalake'"
		},
		"dataprojectDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dataprojectDB'"
		},
		"DataProjectDatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dataprojectstore.dfs.core.windows.net/"
		},
		"dataprojectDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataproject.database.windows.net"
		},
		"dataprojectDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DataProjectDB"
		},
		"dataprojectDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminniranjan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DF-dataproject1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataProjectDatalake_accountKey": {
						"value": "[parameters('DataProjectDatalake_accountKey')]"
					},
					"dataprojectDB_password": {
						"value": "[parameters('dataprojectDB_password')]"
					},
					"DataProjectDatalake_properties_typeProperties_url": {
						"value": "[parameters('DataProjectDatalake_properties_typeProperties_url')]"
					},
					"dataprojectDB_properties_typeProperties_server": {
						"value": "[parameters('dataprojectDB_properties_typeProperties_server')]"
					},
					"dataprojectDB_properties_typeProperties_database": {
						"value": "[parameters('dataprojectDB_properties_typeProperties_database')]"
					},
					"dataprojectDB_properties_typeProperties_userName": {
						"value": "[parameters('dataprojectDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}